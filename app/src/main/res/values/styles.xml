<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="edt_general_style">
        <item name="android:padding">12dp</item>
        <item name="android:color">@color/etechBlue</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textColorHint">@color/etechBlue</item>
        <item name="android:textSize">@dimen/general_font_size</item>
    </style>

    <style name="general_typed_button">
        <item name="android:padding">@dimen/general_padding</item>
        <item name="android:layout_margin">@dimen/general_margin</item>
    </style>

</resources>
